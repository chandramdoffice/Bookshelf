<h1>Listing books</h1>


<table>
  <tr>
    <th>booktitle</th>
    <th>comments</th>
	<th>rating</th>
	
  </tr>
 
  <% @books.each do |book| %>
    <tr>
      <td><%= book.booktitle %></td>
      <td><%= book.comments %></td>
	   <td><%= book.rating %></td>
      <td><%= link_to 'Show', book %></td>
    </tr>
  <% end %>
</table>